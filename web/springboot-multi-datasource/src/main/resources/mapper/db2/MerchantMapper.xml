<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cunchang.mapper.db2.MerchantMapper">

    <select id="findById" resultType="cn.cunchang.pojo.Merchant">
        select id, name, age
        from merchant
        where id = #{id}
    </select>

</mapper>